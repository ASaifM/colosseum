{
    "matches": "../reference/grid-layout-z-order-ref.html",
    "help": [
        "http://www.w3.org/TR/css-grid-1/#z-order"
    ],
    "assert": "the layout should behave the same as reference.",
    "test_case": {
        "tag": "BODY",
        "style": {
            "width": "640px",
            "height": "68px",
            "display": "block"
        },
        "children": [
            {
                "tag": "P",
                "style": {
                    "margin_left": "10px",
                    "margin_top": "10px",
                    "margin_bottom": "10px",
                    "width": "620px",
                    "height": "40px",
                    "display": "block",
                    "margin_right": "10px"
                }
            },
            {
                "tag": "DIV",
                "style": {
                    "border_right_color": "rgb(255, 255, 255)",
                    "border_left_color": "rgb(255, 255, 255)",
                    "border_bottom_color": "rgb(255, 255, 255)",
                    "width": "150px",
                    "height": "18px",
                    "display": "grid",
                    "border_top_color": "rgb(255, 255, 255)"
                },
                "children": [
                    {
                        "tag": "DIV",
                        "style": {
                            "border_right_color": "rgb(255, 255, 255)",
                            "z_index": "1",
                            "border_left_color": "rgb(255, 255, 255)",
                            "border_bottom_color": "rgb(255, 255, 255)",
                            "width": "100px",
                            "height": "18px",
                            "display": "block",
                            "min_height": "auto",
                            "border_top_color": "rgb(255, 255, 255)",
                            "min_width": "auto"
                        }
                    },
                    {
                        "tag": "DIV",
                        "style": {
                            "border_right_color": "rgb(255, 255, 255)",
                            "z_index": "10",
                            "border_left_color": "rgb(255, 255, 255)",
                            "border_bottom_color": "rgb(255, 255, 255)",
                            "width": "100px",
                            "height": "18px",
                            "display": "block",
                            "min_height": "auto",
                            "border_top_color": "rgb(255, 255, 255)",
                            "min_width": "auto"
                        }
                    }
                ]
            }
        ]
    }
}