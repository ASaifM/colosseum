{
    "matches": "../../reference/ref-filled-green-100px-square.xht",
    "assert": "Checks that minimum size for grid items is the specified size regardless of the content size.",
    "help": [
        "http://www.w3.org/TR/css-grid-1/#min-size-auto"
    ],
    "test_case": {
        "tag": "BODY",
        "children": [
            {
                "tag": "DIV",
                "style": {
                    "position": "absolute",
                    "z_index": "-1",
                    "width": "100px",
                    "display": "block",
                    "height": "100px"
                }
            },
            {
                "tag": "DIV",
                "children": [
                    {
                        "tag": "DIV",
                        "children": [
                            {
                                "tag": "DIV",
                                "style": {
                                    "width": "500px",
                                    "display": "block",
                                    "height": "500px"
                                }
                            }
                        ],
                        "style": {
                            "min_height": "auto",
                            "height": "100px",
                            "width": "100px",
                            "display": "block",
                            "min_width": "auto"
                        }
                    }
                ],
                "style": {
                    "width": "10px",
                    "display": "grid",
                    "height": "10px"
                }
            }
        ],
        "style": {
            "margin_bottom": "8px",
            "margin_right": "8px",
            "margin_left": "8px",
            "margin_top": "8px",
            "width": "624px",
            "display": "block",
            "height": "10px"
        }
    }
}