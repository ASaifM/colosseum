{
    "matches": "../../reference/ref-filled-green-100px-square.xht",
    "help": [
        "http://www.w3.org/TR/css-grid-1/#grid-items"
    ],
    "assert": "A grid item is sized within the containing block defined by its grid area",
    "test_case": {
        "tag": "BODY",
        "style": {
            "margin_left": "8px",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "width": "624px",
            "height": "234px",
            "display": "block",
            "margin_right": "8px"
        },
        "children": [
            {
                "tag": "P",
                "style": {
                    "width": "624px",
                    "height": "18px",
                    "margin_bottom": "16px",
                    "margin_top": "16px",
                    "display": "block"
                },
                "children": [
                    {
                        "tag": "STRONG",
                        "style": {}
                    }
                ]
            },
            {
                "tag": "DIV",
                "style": {
                    "width": "200px",
                    "height": "200px",
                    "display": "grid"
                },
                "children": [
                    {
                        "tag": "DIV",
                        "style": {
                            "width": "100px",
                            "height": "100px",
                            "display": "block",
                            "min_height": "auto",
                            "min_width": "auto"
                        },
                        "children": [
                            {
                                "tag": "DIV",
                                "style": {
                                    "width": "100px",
                                    "height": "100px",
                                    "display": "block"
                                }
                            }
                        ]
                    }
                ]
            }
        ]
    }
}