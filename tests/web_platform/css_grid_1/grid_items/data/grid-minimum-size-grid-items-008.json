{
    "assert": "Checks that minimum size for grid items is the specified size for height (regardless the content size) and the transferred size of the image for width (even when content size is bigger).",
    "help": [
        "http://www.w3.org/TR/css-grid-1/#min-size-auto"
    ],
    "test_case": {
        "children": [
            {
                "tag": "DIV",
                "style": {
                    "display": "block",
                    "height": "100px",
                    "position": "absolute",
                    "width": "100px",
                    "z_index": "-1"
                }
            },
            {
                "children": [
                    {
                        "tag": "IMG",
                        "style": {
                            "display": "block",
                            "min_height": "auto",
                            "min_width": "auto",
                            "width": "100px",
                            "height": "100px"
                        }
                    }
                ],
                "tag": "DIV",
                "style": {
                    "display": "grid",
                    "width": "10px",
                    "height": "10px"
                }
            }
        ],
        "tag": "BODY",
        "style": {
            "display": "block",
            "margin_left": "8px",
            "height": "10px",
            "width": "624px",
            "margin_right": "8px",
            "margin_top": "8px",
            "margin_bottom": "8px"
        }
    },
    "matches": "ref-filled-green-100px-square-image.html"
}