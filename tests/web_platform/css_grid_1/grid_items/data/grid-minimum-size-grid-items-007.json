{
    "assert": "Checks that minimum size for grid items is the specified size for width (regardless the content size) and the transferred size of the image for height (even when content size is smaller).",
    "test_case": {
        "children": [
            {
                "tag": "DIV",
                "style": {
                    "display": "block",
                    "width": "100px",
                    "position": "absolute",
                    "z_index": "-1",
                    "height": "100px"
                }
            },
            {
                "children": [
                    {
                        "tag": "IMG",
                        "style": {
                            "display": "block",
                            "width": "50px",
                            "min_height": "auto",
                            "height": "100px",
                            "min_width": "auto"
                        }
                    },
                    {
                        "tag": "IMG",
                        "style": {
                            "display": "block",
                            "width": "50px",
                            "min_height": "auto",
                            "height": "100px",
                            "min_width": "auto"
                        }
                    }
                ],
                "tag": "DIV",
                "style": {
                    "display": "grid",
                    "width": "10px",
                    "height": "10px"
                }
            }
        ],
        "tag": "BODY",
        "style": {
            "display": "block",
            "width": "624px",
            "margin_left": "8px",
            "height": "10px",
            "margin_bottom": "8px",
            "margin_top": "8px",
            "margin_right": "8px"
        }
    },
    "matches": "ref-filled-green-100px-square-image.html",
    "help": [
        "http://www.w3.org/TR/css-grid-1/#min-size-auto"
    ]
}