{
    "matches": "../../reference/ref-filled-green-100px-square.xht",
    "help": [
        "http://www.w3.org/TR/css-grid-1/#grid-containers"
    ],
    "assert": "This test checks that 'inline-grid' value for 'display' property generates an inline level containing box.",
    "test_case": {
        "tag": "BODY",
        "children": [
            {
                "tag": "DIV",
                "style": {
                    "display": "block",
                    "position": "absolute",
                    "height": "100px",
                    "z_index": "-1",
                    "width": "100px"
                }
            },
            {
                "tag": "DIV",
                "style": {
                    "display": "inline-grid",
                    "height": "100px",
                    "width": "50px"
                }
            },
            {
                "tag": "DIV",
                "style": {
                    "display": "inline-grid",
                    "height": "100px",
                    "width": "50px"
                }
            }
        ],
        "style": {
            "display": "block",
            "margin_top": "8px",
            "margin_left": "8px",
            "width": "624px",
            "margin_right": "8px",
            "height": "104px",
            "margin_bottom": "8px"
        }
    }
}