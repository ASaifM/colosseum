{
    "assert": "The property 'display' set to 'table-column' behaves in layout as a table column.",
    "matches": "display-012-ref.xht",
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#propdef-display",
        "http://www.w3.org/TR/CSS21/visuren.html#display-prop"
    ],
    "test_case": {
        "children": [
            {
                "style": {
                    "margin_bottom": "16px",
                    "display": "block",
                    "width": "624px",
                    "margin_top": "16px",
                    "height": "18px"
                },
                "tag": "p"
            },
            {
                "children": [
                    {
                        "style": {
                            "display": "table-column",
                            "width": "0px",
                            "height": "0px"
                        },
                        "tag": "span"
                    },
                    {
                        "style": {
                            "display": "table-column",
                            "width": "0px",
                            "height": "0px"
                        },
                        "tag": "span"
                    },
                    {
                        "children": [
                            {
                                "style": {
                                    "display": "table-cell",
                                    "width": "66.875px",
                                    "height": "18px"
                                },
                                "tag": "div"
                            },
                            {
                                "style": {
                                    "display": "table-cell",
                                    "width": "66.875px",
                                    "height": "18px"
                                },
                                "tag": "div"
                            }
                        ],
                        "style": {
                            "display": "table-row",
                            "width": "133.75px",
                            "height": "18px"
                        },
                        "tag": "div"
                    }
                ],
                "style": {
                    "display": "table",
                    "width": "133.75px",
                    "height": "18px"
                },
                "tag": "div"
            }
        ],
        "style": {
            "margin_bottom": "8px",
            "display": "block",
            "margin_top": "8px",
            "margin_left": "8px",
            "margin_right": "8px",
            "height": "52px",
            "width": "624px"
        },
        "tag": "body"
    }
}