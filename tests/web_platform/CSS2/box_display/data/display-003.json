{
    "matches": null,
    "assert": "The property 'display' set to 'list-item' behaves in layout as a list item block.",
    "test_case": {
        "tag": "body",
        "style": {
            "margin_top": "8px",
            "margin_left": "50px",
            "display": "block",
            "margin_right": "8px",
            "height": "52px",
            "margin_bottom": "8px",
            "width": "582px"
        },
        "children": [
            {
                "tag": "p",
                "style": {
                    "height": "18px",
                    "margin_top": "16px",
                    "display": "block",
                    "margin_bottom": "16px",
                    "width": "582px"
                }
            },
            {
                "tag": "div",
                "style": {
                    "height": "18px",
                    "display": "block",
                    "width": "582px"
                },
                "children": [
                    {
                        "tag": "span",
                        "style": {
                            "height": "18px",
                            "display": "list-item",
                            "width": "582px"
                        }
                    }
                ]
            }
        ]
    },
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#propdef-display",
        "http://www.w3.org/TR/CSS21/visuren.html#display-prop"
    ]
}