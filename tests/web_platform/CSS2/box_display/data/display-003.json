{
    "assert": "The property 'display' set to 'list-item' behaves in layout as a list item block.",
    "matches": null,
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#propdef-display",
        "http://www.w3.org/TR/CSS21/visuren.html#display-prop"
    ],
    "test_case": {
        "children": [
            {
                "style": {
                    "margin_bottom": "16px",
                    "display": "block",
                    "width": "582px",
                    "margin_top": "16px",
                    "height": "18px"
                },
                "tag": "p"
            },
            {
                "children": [
                    {
                        "style": {
                            "display": "list-item",
                            "width": "582px",
                            "height": "18px"
                        },
                        "tag": "span"
                    }
                ],
                "style": {
                    "display": "block",
                    "width": "582px",
                    "height": "18px"
                },
                "tag": "div"
            }
        ],
        "style": {
            "margin_bottom": "8px",
            "display": "block",
            "margin_top": "8px",
            "margin_left": "50px",
            "margin_right": "8px",
            "height": "52px",
            "width": "582px"
        },
        "tag": "body"
    }
}