{
    "matches": "display-001-ref.xht",
    "assert": "The property 'display' set to 'table-cell' behaves in layout as a table cell.",
    "test_case": {
        "style": {
            "width": "624px",
            "margin_bottom": "8px",
            "height": "18px",
            "margin_top": "8px",
            "margin_right": "8px",
            "margin_left": "8px",
            "display": "block"
        },
        "tag": "body",
        "children": [
            {
                "style": {
                    "width": "129.3125px",
                    "height": "18px",
                    "display": "table"
                },
                "tag": "div",
                "children": [
                    {
                        "style": {
                            "width": "129.3125px",
                            "height": "18px",
                            "display": "table-row"
                        },
                        "tag": "div",
                        "children": [
                            {
                                "style": {
                                    "width": "66.65625px",
                                    "height": "18px",
                                    "display": "table-cell"
                                },
                                "tag": "div",
                                "children": [
                                    {
                                        "style": {},
                                        "text": "Filler text"
                                    }
                                ]
                            },
                            {
                                "style": {
                                    "width": "62.65625px",
                                    "height": "18px",
                                    "display": "table-cell"
                                },
                                "tag": "div",
                                "children": [
                                    {
                                        "style": {},
                                        "text": "Filler text"
                                    }
                                ]
                            }
                        ]
                    }
                ]
            }
        ]
    },
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#propdef-display",
        "http://www.w3.org/TR/CSS21/visuren.html#display-prop"
    ]
}