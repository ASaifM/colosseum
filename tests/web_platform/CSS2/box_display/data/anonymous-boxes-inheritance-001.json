            "margin_bottom": "8px",
            "height": "200px",
            "margin_top": "8px",
            "margin_right": "8px",
            "margin_left": "8px",
            "display": "block"
        },
        "tag": "body",
        "children": [
            {
                "style": {
                    "width": "624px",
                    "border_top_color": "rgb(0, 0, 255)",
                    "border_bottom_color": "rgb(0, 0, 255)",
                    "border_right_color": "rgb(0, 0, 255)",
                    "border_left_color": "rgb(0, 0, 255)",
                    "height": "200px",
                    "display": "block"
                },
                "tag": "div",
                "children": [
                    {
                        "style": {},
                        "text": "X"
                    },
                    {
                        "style": {
                            "width": "624px",
                            "border_top_color": "rgb(255, 165, 0)",
                            "border_bottom_color": "rgb(255, 165, 0)",
                            "border_right_color": "rgb(255, 165, 0)",
                            "border_left_color": "rgb(255, 165, 0)",
                            "height": "100px",
                            "display": "block"
                        },
                        "tag": "div",
                        "children": [
                            {
                                "style": {},
                                "text": "X"
                            }
                        ]
                    }
                ]
            }
        ]
    },
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#anonymous-block-level"
    ]
