{
    "matches": "display-001-ref.xht",
    "assert": "The property 'display' set to 'inline-table' behaves in layout as an inline table.",
    "test_case": {
        "style": {
            "width": "624px",
            "margin_bottom": "8px",
            "height": "18px",
            "margin_top": "8px",
            "margin_right": "8px",
            "margin_left": "8px",
            "display": "block"
        },
        "tag": "body",
        "children": [
            {
                "style": {
                    "width": "624px",
                    "height": "18px",
                    "display": "block"
                },
                "tag": "div",
                "children": [
                    {
                        "style": {},
                        "text": "Filler text"
                    },
                    {
                        "style": {
                            "width": "62.671875px",
                            "height": "18px",
                            "display": "inline-table"
                        },
                        "tag": "div",
                        "children": [
                            {
                                "style": {
                                    "width": "62.671875px",
                                    "height": "18px",
                                    "display": "table-row"
                                },
                                "tag": "div",
                                "children": [
                                    {
                                        "style": {
                                            "width": "38.671875px",
                                            "height": "18px",
                                            "display": "table-cell"
                                        },
                                        "tag": "div",
                                        "children": [
                                            {
                                                "style": {},
                                                "text": "Filler"
                                            }
                                        ]
                                    },
                                    {
                                        "style": {
                                            "width": "24px",
                                            "height": "18px",
                                            "display": "table-cell"
                                        },
                                        "tag": "div",
                                        "children": [
                                            {
                                                "style": {},
                                                "text": "text"
                                            }
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                ]
            }
        ]
    },
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#propdef-display",
        "http://www.w3.org/TR/CSS21/visuren.html#display-prop"
    ]
}