{
    "assert": "Elements set to 'display: none' do not render even if element is positioned absolute.",
    "matches": "../reference/ref-if-there-is-no-red.xht",
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#dis-pos-flo"
    ],
    "test_case": {
        "children": [
            {
                "children": [
                    {
                        "style": {},
                        "tag": "strong"
                    }
                ],
                "style": {
                    "margin_bottom": "16px",
                    "display": "block",
                    "width": "624px",
                    "margin_top": "16px",
                    "height": "18px"
                },
                "tag": "p"
            },
            {
                "style": {
                    "position": "absolute",
                    "display": "none"
                },
                "tag": "div"
            }
        ],
        "style": {
            "margin_bottom": "8px",
            "display": "block",
            "margin_top": "8px",
            "margin_left": "8px",
            "margin_right": "8px",
            "height": "18px",
            "width": "624px"
        },
        "tag": "body"
    }
}