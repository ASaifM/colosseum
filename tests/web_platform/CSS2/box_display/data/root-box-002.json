{
    "matches": "root-box-002-ref.xht",
    "assert": "HTML elements can be set to other display     property values - like sub-table elements just like in this testcase -     in order to behave like another element.",
    "test_case": {
        "style": {
            "width": "200px",
            "height": "100px",
            "display": "table-row"
        },
        "tag": "body",
        "children": [
            {
                "style": {
                    "width": "100px",
                    "height": "100px",
                    "display": "table-cell"
                },
                "tag": "div"
            },
            {
                "style": {
                    "width": "100px",
                    "height": "100px",
                    "display": "table-cell"
                },
                "tag": "div"
            }
        ]
    },
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#display-prop"
    ]
}