{
    "assert": "Relatively positioned element with auto positioning appear as though they were positioned in flow.",
    "matches": "position-relative-028-ref.xht",
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#comp-relpos"
    ],
    "test_case": {
        "style": {
            "height": "52px",
            "display": "block",
            "margin_right": "8px",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "width": "624px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "margin_bottom": "16px",
                    "width": "624px",
                    "height": "18px",
                    "display": "block",
                    "margin_top": "16px"
                },
                "tag": "p"
            },
            {
                "style": {
                    "width": "480px",
                    "height": "18px",
                    "display": "block"
                },
                "children": [
                    {
                        "style": {
                            "position": "relative"
                        },
                        "tag": "span"
                    }
                ],
                "tag": "div"
            }
        ],
        "tag": "body"
    }
}