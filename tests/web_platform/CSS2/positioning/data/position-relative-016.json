{
    "matches": "position-relative-016-ref.xht",
    "test_case": {
        "children": [
            {
                "style": {
                    "border_bottom_color": "rgba(0, 0, 0, 0)",
                    "border_left_width": "1px",
                    "border_left_style": "solid",
                    "border_right_color": "rgba(0, 0, 0, 0)",
                    "border_top_style": "solid",
                    "height": "0px",
                    "width": "96px",
                    "border_bottom_width": "1px",
                    "border_top_color": "rgba(0, 0, 0, 0)",
                    "border_right_width": "1px",
                    "display": "block",
                    "border_left_color": "rgba(0, 0, 0, 0)",
                    "border_top_width": "1px",
                    "border_right_style": "solid",
                    "border_bottom_style": "solid"
                },
                "tag": "div"
            },
            {
                "style": {
                    "position": "absolute",
                    "height": "96px",
                    "display": "block",
                    "margin_top": "96px",
                    "width": "96px"
                },
                "tag": "div"
            },
            {
                "children": [
                    {
                        "style": {
                            "position": "relative",
                            "height": "96px",
                            "display": "block",
                            "width": "96px"
                        },
                        "tag": "div"
                    }
                ],
                "style": {
                    "position": "relative",
                    "height": "96px",
                    "display": "block",
                    "width": "96px",
                    "bottom": "-96px"
                },
                "tag": "div"
            }
        ],
        "style": {
            "margin_left": "8px",
            "height": "98px",
            "display": "block",
            "margin_top": "8px",
            "margin_right": "8px",
            "margin_bottom": "8px",
            "width": "624px"
        },
        "tag": "body"
    },
    "assert": "Used value of top equals negative bottom when top is auto and bottom is given. (Computed value of top remains auto.)",
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#relative-positioning"
    ]
}