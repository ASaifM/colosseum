{
    "assert": "Absolute positioned elements are taken out of the normal flow when they have explicit offsets.",
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#absolute-positioning"
    ],
    "test_case": {
        "children": [
            {
                "children": [
                    {
                        "style": {
                            "display": "block",
                            "height": "100px",
                            "width": "100px"
                        },
                        "tag": "div"
                    },
                    {
                        "style": {
                            "top": "0px",
                            "height": "100px",
                            "display": "block",
                            "left": "0px",
                            "width": "100px",
                            "position": "absolute"
                        },
                        "tag": "div"
                    }
                ],
                "style": {
                    "display": "block",
                    "height": "100px",
                    "width": "200px",
                    "position": "relative"
                },
                "tag": "div"
            }
        ],
        "style": {
            "margin_top": "8px",
            "height": "100px",
            "margin_right": "8px",
            "width": "624px",
            "display": "block",
            "margin_left": "8px",
            "margin_bottom": "8px"
        },
        "tag": "body"
    },
    "matches": "../reference/ref-filled-green-100px-square.xht"
}