{
    "matches": "position-relative-005-ref.xht",
    "assert": "Relatively positioned element with 'left' set to 'auto' and 'right' set to a value appears at expected offset.",
    "test_case": {
        "tag": "body",
        "children": [
            {
                "tag": "p",
                "style": {
                    "margin_top": "16px",
                    "margin_bottom": "16px",
                    "width": "624px",
                    "height": "18px",
                    "display": "block"
                }
            },
            {
                "tag": "div",
                "children": [
                    {
                        "tag": "div",
                        "style": {
                            "width": "96px",
                            "height": "96px",
                            "display": "block"
                        }
                    },
                    {
                        "tag": "div",
                        "style": {
                            "position": "relative",
                            "right": "96px",
                            "width": "96px",
                            "display": "block",
                            "height": "96px",
                            "top": "-96px"
                        }
                    }
                ],
                "style": {
                    "width": "96px",
                    "height": "96px",
                    "display": "block",
                    "margin_left": "96px"
                }
            }
        ],
        "style": {
            "width": "624px",
            "margin_right": "8px",
            "display": "block",
            "margin_left": "8px",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "130px"
        }
    },
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#relative-positioning"
    ]
}