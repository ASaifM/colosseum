{
    "matches": "position-relative-005-ref.xht",
    "assert": "If the 'direction' property of the containing block is 'rtl', the value of 'right' wins and 'left' becomes negative 'right'.",
    "test_case": {
        "tag": "body",
        "children": [
            {
                "tag": "p",
                "style": {
                    "margin_top": "16px",
                    "margin_bottom": "16px",
                    "width": "624px",
                    "height": "18px",
                    "display": "block"
                }
            },
            {
                "tag": "div",
                "children": [
                    {
                        "tag": "div",
                        "style": {
                            "position": "relative",
                            "right": "96px",
                            "width": "96px",
                            "direction": "rtl",
                            "display": "block",
                            "left": "96px",
                            "height": "96px"
                        }
                    }
                ],
                "style": {
                    "height": "96px",
                    "width": "192px",
                    "direction": "rtl",
                    "display": "block"
                }
            }
        ],
        "style": {
            "width": "624px",
            "margin_right": "8px",
            "display": "block",
            "margin_left": "8px",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "130px"
        }
    },
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#relative-positioning"
    ]
}