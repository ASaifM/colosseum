{
    "assert": "Fixed position elements cannot be paginated.",
    "test_case": {
        "style": {
            "display": "block",
            "height": "480px",
            "width": "640px"
        },
        "children": [
            {
                "style": {
                    "padding_left": "16px",
                    "display": "block",
                    "padding_top": "16px",
                    "height": "18px",
                    "width": "624px"
                },
                "children": [
                    {
                        "style": {},
                        "text": "PREREQUISITE: Switch to print preview or a paged view."
                    }
                ],
                "tag": "p"
            },
            {
                "style": {
                    "padding_left": "16px",
                    "display": "block",
                    "padding_top": "16px",
                    "height": "36px",
                    "width": "624px"
                },
                "children": [
                    {
                        "style": {},
                        "text": "Test passes if there is only one page and there is no red underlined text visible when paginated or printed."
                    }
                ],
                "tag": "p"
            },
            {
                "style": {
                    "height": "480px",
                    "position": "fixed",
                    "width": "481.4375px",
                    "z_index": "0",
                    "display": "block",
                    "top": "80px"
                },
                "children": [
                    {
                        "style": {
                            "display": "block",
                            "height": "480px",
                            "width": "481.4375px"
                        },
                        "tag": "div"
                    },
                    {
                        "style": {
                            "border_right_color": "rgb(255, 0, 0)",
                            "border_left_color": "rgb(255, 0, 0)",
                            "border_top_color": "rgb(255, 0, 0)",
                            "border_bottom_color": "rgb(255, 0, 0)"
                        },
                        "children": [
                            {
                                "style": {},
                                "text": "Test fails if this line of text is visible when the page is paginated or printed."
                            }
                        ],
                        "tag": "span"
                    }
                ],
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#propdef-position",
        "http://www.w3.org/TR/CSS21/visuren.html#choose-position"
    ],
    "matches": null
}