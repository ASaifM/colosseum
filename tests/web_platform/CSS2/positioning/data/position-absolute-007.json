{
    "matches": "position-absolute-007-ref.xht",
    "assert": "The contents of an absolutely positioned element do not flow around any other boxes and can obscure the contents of another box.",
    "test_case": {
        "tag": "body",
        "children": [
            {
                "tag": "p",
                "style": {
                    "margin_top": "16px",
                    "margin_bottom": "16px",
                    "width": "624px",
                    "height": "36px",
                    "display": "block"
                }
            },
            {
                "tag": "div",
                "children": [
                    {
                        "tag": "div",
                        "style": {
                            "position": "absolute",
                            "width": "96px",
                            "height": "96px",
                            "display": "block",
                            "top": "48px"
                        }
                    },
                    {
                        "tag": "div",
                        "style": {
                            "height": "192px",
                            "width": "48px",
                            "float": "left",
                            "display": "block"
                        }
                    }
                ],
                "style": {
                    "position": "relative",
                    "width": "624px",
                    "height": "0px",
                    "display": "block"
                }
            }
        ],
        "style": {
            "width": "624px",
            "margin_right": "8px",
            "display": "block",
            "margin_left": "8px",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "36px"
        }
    },
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#absolute-positioning"
    ]
}