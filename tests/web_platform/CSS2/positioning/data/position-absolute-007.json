{
    "assert": "The contents of an absolutely positioned element do not flow around any other boxes and can obscure the contents of another box.",
    "matches": "position-absolute-007-ref.xht",
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#absolute-positioning"
    ],
    "test_case": {
        "style": {
            "height": "36px",
            "display": "block",
            "margin_right": "8px",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "width": "624px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "margin_bottom": "16px",
                    "width": "624px",
                    "height": "36px",
                    "display": "block",
                    "margin_top": "16px"
                },
                "tag": "p"
            },
            {
                "style": {
                    "position": "relative",
                    "width": "624px",
                    "height": "0px",
                    "display": "block"
                },
                "children": [
                    {
                        "style": {
                            "position": "absolute",
                            "width": "96px",
                            "height": "96px",
                            "display": "block",
                            "top": "48px"
                        },
                        "tag": "div"
                    },
                    {
                        "style": {
                            "float": "left",
                            "height": "192px",
                            "display": "block",
                            "width": "48px"
                        },
                        "tag": "div"
                    }
                ],
                "tag": "div"
            }
        ],
        "tag": "body"
    }
}