{
    "assert": "An absolutely positioned inline replaced element with only one value as 'auto' solves for that value (margin-bottom).",
    "test_case": {
        "style": {
            "margin_top": "8px",
            "height": "18px",
            "margin_bottom": "8px",
            "margin_right": "8px",
            "display": "block",
            "width": "624px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "height": "15px",
                    "position": "relative",
                    "width": "624px",
                    "border_top_style": "solid",
                    "display": "block",
                    "border_top_width": "3px"
                },
                "children": [
                    {
                        "style": {
                            "margin_top": "48px",
                            "position": "absolute",
                            "margin_bottom": "-192px",
                            "width": "15px",
                            "display": "block",
                            "top": "48px",
                            "bottom": "96px",
                            "height": "15px"
                        },
                        "tag": "img"
                    },
                    {
                        "style": {
                            "position": "relative",
                            "left": "15px",
                            "width": "15px",
                            "display": "block",
                            "top": "96px",
                            "height": "15px"
                        },
                        "tag": "div"
                    }
                ],
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#abs-replaced-height"
    ],
    "matches": "absolute-replaced-height-008-ref.xht"
