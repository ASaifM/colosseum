{
    "assert": "Equal 50% positive and negative positioning can vertically center objects.",
    "matches": "position-relative-nested-001-ref.xht",
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#positioning-scheme"
    ],
    "test_case": {
        "style": {
            "height": "334px",
            "display": "block",
            "margin_right": "8px",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "width": "624px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "margin_bottom": "16px",
                    "width": "624px",
                    "height": "18px",
                    "display": "block",
                    "margin_top": "16px"
                },
                "children": [
                    {
                        "style": {},
                        "tag": "strong"
                    }
                ],
                "tag": "p"
            },
            {
                "style": {
                    "width": "200px",
                    "height": "300px",
                    "display": "block"
                },
                "children": [
                    {
                        "style": {
                            "position": "relative",
                            "width": "200px",
                            "height": "100px",
                            "display": "block",
                            "top": "50%"
                        },
                        "children": [
                            {
                                "style": {
                                    "position": "relative",
                                    "width": "200px",
                                    "height": "150px",
                                    "display": "block",
                                    "top": "-50%"
                                },
                                "tag": "div"
                            }
                        ],
                        "tag": "div"
                    }
                ],
                "tag": "div"
            }
        ],
        "tag": "body"
    }
}