{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#inline-replaced-height"
    ],
    "test_case": {
        "children": [
            {
                "children": [
                    {
                        "tag": "img",
                        "style": {
                            "height": "96px",
                            "width": "96px"
                        }
                    },
                    {
                        "tag": "div",
                        "style": {
                            "display": "block",
                            "height": "96px",
                            "position": "absolute",
                            "width": "96px",
                            "top": "0px",
                            "left": "96px"
                        }
                    }
                ],
                "tag": "div",
                "style": {
                    "display": "block",
                    "height": "96px",
                    "position": "relative",
                    "width": "624px"
                }
            }
        ],
        "tag": "body",
        "style": {
            "margin_top": "8px",
            "display": "block",
            "width": "624px",
            "margin_right": "8px",
            "margin_bottom": "8px",
            "height": "96px",
            "margin_left": "8px"
        }
    },
    "assert": "The 'height' is the used width divided by the intrinsic ratio when an inline-block replaced element has an intrinsic ratio, 'height' is set to 'auto' and 'width' is specified.",
    "matches": "inline-block-replaced-height-003-ref.xht"
}