{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#inline-replaced-height"
    ],
    "test_case": {
        "style": {
            "width": "624px",
            "display": "block",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "96px",
            "margin_right": "8px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "width": "624px",
                    "display": "block",
                    "height": "96px",
                    "position": "relative"
                },
                "children": [
                    {
                        "style": {
                            "width": "96px",
                            "height": "96px"
                        },
                        "tag": "img"
                    },
                    {
                        "style": {
                            "width": "96px",
                            "display": "block",
                            "top": "0px",
                            "height": "96px",
                            "left": "96px",
                            "position": "absolute"
                        },
                        "tag": "div"
                    }
                ],
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "matches": "inline-block-replaced-height-003-ref.xht",
    "assert": "The 'height' is the used width divided by the intrinsic ratio when an inline-block replaced element has an intrinsic ratio, 'height' is set to 'auto' and 'width' is specified."
}