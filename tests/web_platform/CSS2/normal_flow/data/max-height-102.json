{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#propdef-max-height",
        "http://www.w3.org/TR/CSS21/visudet.html#min-max-heights"
    ],
    "test_case": {
        "style": {
            "width": "624px",
            "display": "block",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "0px",
            "margin_right": "8px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "width": "624px",
                    "display": "block",
                    "max_height": "0px",
                    "height": "0px"
                },
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "matches": "../reference/ref-if-there-is-no-red.xht",
    "assert": "The 'max-height' property sets a positive zero length value with no units."
}