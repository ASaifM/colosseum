{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#inline-replaced-height"
    ],
    "test_case": {
        "children": [
            {
                "children": [
                    {
                        "tag": "img",
                        "style": {
                            "display": "inline-block",
                            "height": "15px",
                            "width": "15px"
                        }
                    },
                    {
                        "tag": "div",
                        "style": {
                            "display": "block",
                            "height": "15px",
                            "position": "absolute",
                            "width": "15px",
                            "top": "0px",
                            "left": "15px"
                        }
                    }
                ],
                "tag": "div",
                "style": {
                    "display": "block",
                    "height": "15px",
                    "position": "relative",
                    "width": "624px"
                }
            }
        ],
        "tag": "body",
        "style": {
            "margin_top": "8px",
            "display": "block",
            "width": "624px",
            "margin_right": "8px",
            "margin_bottom": "8px",
            "height": "15px",
            "margin_left": "8px"
        }
    },
    "assert": "The used value of 'height' is the intrinsic height when an inline-block replaced element has an intrinsic height and 'width' computed as 'auto'.",
    "matches": "block-replaced-height-002-ref.xht"
}