{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#normal-block"
    ],
    "test_case": {
        "children": [
            {
                "children": [
                    {
                        "tag": "div",
                        "style": {
                            "display": "block",
                            "height": "96px",
                            "float": "left",
                            "width": "96px"
                        }
                    }
                ],
                "tag": "div",
                "style": {
                    "display": "block",
                    "height": "0px",
                    "width": "624px"
                }
            }
        ],
        "tag": "body",
        "style": {
            "margin_top": "8px",
            "display": "block",
            "width": "624px",
            "margin_right": "8px",
            "margin_bottom": "8px",
            "height": "0px",
            "margin_left": "8px"
        }
    },
    "assert": "A floated child element is not considered when sizing the 'height' of a block-level non-replaced element is in normal flow.",
    "matches": "../reference/ref-if-there-is-no-red.xht"
}