{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#inlineblock-replaced-width"
    ],
    "test_case": {
        "children": [
            {
                "children": [
                    {
                        "children": [
                            {
                                "tag": "svg:rect",
                                "style": {
                                    "height": "300px",
                                    "width": "600px"
                                }
                            }
                        ],
                        "tag": "svg:svg",
                        "style": {
                            "height": "300px",
                            "width": "300px"
                        }
                    },
                    {
                        "tag": "div",
                        "style": {
                            "display": "block",
                            "width": "300px",
                            "position": "relative",
                            "bottom": "300px",
                            "height": "300px"
                        }
                    }
                ],
                "tag": "div",
                "style": {
                    "display": "block",
                    "height": "300px",
                    "width": "600px"
                }
            }
        ],
        "tag": "body",
        "style": {
            "margin_top": "8px",
            "display": "block",
            "width": "624px",
            "margin_right": "8px",
            "margin_bottom": "8px",
            "height": "300px",
            "margin_left": "8px"
        }
    },
    "assert": "If an inline-block replaced element (like the svg element in this test) has no intrinsic width and no intrinsic ratio, then the used value of 'width' becomes 300px.",
    "matches": "../positioning/absolute-replaced-width-003c-ref.xht"
}