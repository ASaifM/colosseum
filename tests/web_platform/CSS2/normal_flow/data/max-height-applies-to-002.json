{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#propdef-max-height",
        "http://www.w3.org/TR/CSS21/visudet.html#min-max-heights"
    ],
    "test_case": {
        "style": {
            "width": "624px",
            "display": "block",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "18px",
            "margin_right": "8px",
            "margin_left": "8px"
        },
        "children": [
            {
                "text": "If 'max-height' is supported on 'table-header-group' then a square will be visible.",
                "style": {}
            },
            {
                "style": {
                    "width": "96px",
                    "display": "table",
                    "height": "18px"
                },
                "children": [
                    {
                        "style": {
                            "width": "96px",
                            "display": "table-header-group",
                            "max_height": "96px",
                            "height": "18px"
                        },
                        "children": [
                            {
                                "style": {
                                    "width": "96px",
                                    "display": "table-row",
                                    "height": "18px"
                                },
                                "children": [
                                    {
                                        "style": {
                                            "width": "96px",
                                            "display": "table-cell",
                                            "height": "18px"
                                        },
                                        "children": [],
                                        "tag": "div"
                                    }
                                ],
                                "tag": "div"
                            }
                        ],
                        "tag": "div"
                    }
                ],
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "matches": null,
    "assert": "The 'max-height' property applies to elements with 'display' set to 'table-header-group'."
}