{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#inline-replaced-height",
        "http://www.w3.org/TR/CSS21/visudet.html#min-max-widths"
    ],
    "test_case": {
        "style": {
            "width": "624px",
            "display": "block",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "100px",
            "margin_right": "8px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "width": "100px",
                    "z_index": "-1",
                    "display": "block",
                    "height": "104px",
                    "position": "absolute"
                },
                "children": [
                    {
                        "style": {
                            "width": "100px",
                            "height": "100px",
                            "max_width": "100px"
                        },
                        "tag": "img"
                    }
                ],
                "tag": "div"
            },
            {
                "style": {
                    "width": "100px",
                    "display": "block",
                    "height": "100px"
                },
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "matches": "../reference/ref-filled-green-100px-square.xht",
    "assert": "The height of an inline replaced element should be re-calculated based on its constrained width (constrained by computed 'max-width' value) and based its intrinsic ratio when it has an intrinsic ratio."
}