{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#inline-replaced-height"
    ],
    "test_case": {
        "style": {
            "width": "624px",
            "display": "block",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "104px",
            "margin_right": "8px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "display": "block",
                    "border_left_color": "rgb(0, 128, 0)",
                    "border_bottom_style": "solid",
                    "width": "60px",
                    "border_top_width": "20px",
                    "border_bottom_width": "20px",
                    "border_top_color": "rgb(0, 128, 0)",
                    "border_top_style": "solid",
                    "position": "absolute",
                    "border_right_style": "solid",
                    "border_left_width": "20px",
                    "border_left_style": "solid",
                    "border_bottom_color": "rgb(0, 128, 0)",
                    "height": "60px",
                    "border_right_width": "20px",
                    "border_right_color": "rgb(0, 128, 0)"
                },
                "tag": "div"
            },
            {
                "style": {
                    "width": "624px",
                    "display": "block",
                    "height": "104px"
                },
                "children": [
                    {
                        "style": {
                            "border_left_color": "rgb(255, 0, 0)",
                            "border_bottom_style": "solid",
                            "width": "60px",
                            "border_top_width": "20px",
                            "border_bottom_width": "20px",
                            "border_top_color": "rgb(255, 0, 0)",
                            "border_top_style": "solid",
                            "border_right_style": "solid",
                            "border_left_width": "20px",
                            "border_left_style": "solid",
                            "border_bottom_color": "rgb(255, 0, 0)",
                            "height": "60px",
                            "border_right_width": "20px",
                            "border_right_color": "rgb(255, 0, 0)"
                        },
                        "children": [
                            {
                                "text": "FAIL: SVG support required",
                                "style": {}
                            }
                        ],
                        "tag": "object"
                    }
                ],
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "matches": "../reference/ref-filled-green-100px-square.xht",
    "assert": "An inline replaced element with an intrinsic height only     and a specified width is drawn at the specified width and intrinsic height."
}