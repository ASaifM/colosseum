{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#inline-replaced-height"
    ],
    "test_case": {
        "style": {
            "width": "624px",
            "display": "block",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "192px",
            "margin_right": "8px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "width": "624px",
                    "display": "block",
                    "height": "192px",
                    "position": "relative"
                },
                "children": [
                    {
                        "style": {
                            "border_left_color": "rgb(255, 0, 0)",
                            "border_bottom_style": "solid",
                            "width": "300px",
                            "border_top_width": "3px",
                            "border_bottom_width": "3px",
                            "border_top_color": "rgb(255, 0, 0)",
                            "border_top_style": "solid",
                            "border_right_style": "solid",
                            "border_left_width": "3px",
                            "border_left_style": "solid",
                            "border_bottom_color": "rgb(255, 0, 0)",
                            "height": "96px",
                            "border_right_width": "3px",
                            "border_right_color": "rgb(255, 0, 0)"
                        },
                        "tag": "iframe"
                    },
                    {
                        "style": {
                            "display": "block",
                            "border_left_color": "rgb(0, 128, 0)",
                            "border_bottom_style": "solid",
                            "width": "300px",
                            "border_top_width": "3px",
                            "border_bottom_width": "3px",
                            "border_top_color": "rgb(0, 128, 0)",
                            "border_top_style": "solid",
                            "position": "absolute",
                            "border_right_style": "solid",
                            "border_left_width": "3px",
                            "top": "0px",
                            "border_left_style": "solid",
                            "border_bottom_color": "rgb(0, 128, 0)",
                            "height": "96px",
                            "border_right_width": "3px",
                            "border_right_color": "rgb(0, 128, 0)"
                        },
                        "tag": "div"
                    }
                ],
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "matches": "block-replaced-height-005-ref.xht",
    "assert": "An inline replaced element with percentage intrinsic height resolves based on the containing block height when percentage is explicitly specified."
}