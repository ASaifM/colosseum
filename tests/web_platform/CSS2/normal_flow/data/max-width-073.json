{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#propdef-max-width",
        "http://www.w3.org/TR/CSS21/visudet.html#min-max-widths"
    ],
    "test_case": {
        "children": [
            {
                "tag": "div",
                "style": {
                    "display": "block",
                    "max_width": "120px",
                    "width": "120px",
                    "height": "96px"
                }
            },
            {
                "tag": "div",
                "style": {
                    "margin_top": "4px",
                    "display": "block",
                    "border_left_style": "solid",
                    "border_left_width": "120px",
                    "width": "504px",
                    "height": "96px"
                }
            }
        ],
        "tag": "body",
        "style": {
            "margin_top": "8px",
            "display": "block",
            "width": "624px",
            "margin_right": "8px",
            "margin_bottom": "8px",
            "height": "196px",
            "margin_left": "8px"
        }
    },
    "assert": "The 'max-width' property sets a positive nominal length value in 'em' units.",
    "matches": "max-width-072-ref.xht"
}