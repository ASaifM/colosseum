{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#propdef-height",
        "http://www.w3.org/TR/CSS21/visudet.html#the-height-property"
    ],
    "test_case": {
        "style": {
            "width": "624px",
            "display": "block",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "288px",
            "margin_right": "8px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "width": "624px",
                    "display": "block",
                    "height": "288px",
                    "position": "relative"
                },
                "children": [
                    {
                        "style": {
                            "width": "96px",
                            "display": "block",
                            "height": "288px",
                            "position": "relative"
                        },
                        "tag": "div"
                    },
                    {
                        "style": {
                            "width": "96px",
                            "display": "block",
                            "top": "0px",
                            "border_top_width": "288px",
                            "height": "0px",
                            "left": "100px",
                            "border_top_style": "solid",
                            "position": "absolute"
                        },
                        "tag": "div"
                    }
                ],
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "matches": "height-061-ref.xht",
    "assert": "The 'height' property sets a positive nominal length value in inches."
}