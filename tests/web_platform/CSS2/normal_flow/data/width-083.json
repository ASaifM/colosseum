            {
                "tag": "div",
                "style": {
                    "margin_top": "4px",
                    "display": "block",
                    "border_left_style": "solid",
                    "border_left_width": "96px",
                    "width": "528px",
                    "height": "96px"
                }
            }
        ],
        "tag": "body",
        "style": {
            "margin_top": "8px",
            "display": "block",
            "width": "624px",
            "margin_right": "8px",
            "margin_bottom": "8px",
            "height": "196px",
            "margin_left": "8px"
        }
    },
    "assert": "The 'width' property sets a nominal length value in 'ex' units.",
    "matches": "max-width-006-ref.xht"
}