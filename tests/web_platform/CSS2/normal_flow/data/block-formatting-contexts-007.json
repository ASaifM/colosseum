                },
                "children": [
                    {
                        "style": {
                            "border_left_color": "rgb(255, 165, 0)",
                            "display": "block",
                            "border_left_width": "5px",
                            "width": "0px",
                            "border_left_style": "solid",
                            "float": "left",
                            "height": "96px"
                        },
                        "tag": "div"
                    }
                ],
                "tag": "div"
            }
        ],
        "tag": "body"
