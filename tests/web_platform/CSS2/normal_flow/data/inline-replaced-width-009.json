{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#inline-replaced-width"
    ],
    "test_case": {
        "style": {
            "width": "624px",
            "display": "block",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "300px",
            "margin_right": "8px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "width": "600px",
                    "display": "block",
                    "height": "300px"
                },
                "children": [
                    {
                        "style": {
                            "width": "300px",
                            "height": "300px"
                        },
                        "children": [
                            {
                                "style": {
                                    "width": "600px",
                                    "height": "300px"
                                },
                                "tag": "svg:rect"
                            }
                        ],
                        "tag": "svg:svg"
                    },
                    {
                        "style": {
                            "width": "300px",
                            "display": "block",
                            "bottom": "300px",
                            "height": "300px",
                            "position": "relative"
                        },
                        "tag": "div"
                    }
                ],
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "matches": "../positioning/absolute-replaced-width-003c-ref.xht",
    "assert": "If an inline replaced element (like the svg element in this test) has no intrinsic width and no intrinsic ratio, then the used value of 'width' becomes 300px."
}