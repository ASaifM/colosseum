{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#min-max-widths"
    ],
    "test_case": {
        "style": {
            "width": "624px",
            "display": "block",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "192px",
            "margin_right": "8px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "width": "192px",
                    "display": "block",
                    "height": "192px"
                },
                "children": [
                    {
                        "style": {
                            "width": "96px",
                            "display": "block",
                            "max_width": "50%",
                            "height": "96px"
                        },
                        "tag": "div"
                    },
                    {
                        "style": {
                            "width": "96px",
                            "display": "block",
                            "height": "96px"
                        },
                        "tag": "div"
                    }
                ],
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "matches": "max-width-percentage-001-ref.xht",
    "assert": "The percentage is calculated in relation to the containing block."
}