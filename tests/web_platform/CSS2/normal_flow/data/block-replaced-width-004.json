{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#block-replaced-width"
    ],
    "test_case": {
        "style": {
            "width": "624px",
            "display": "block",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "294px",
            "margin_right": "8px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "border_right_style": "solid",
                    "width": "288px",
                    "border_top_width": "3px",
                    "border_left_width": "3px",
                    "height": "288px",
                    "border_left_style": "solid",
                    "display": "block",
                    "border_bottom_width": "3px",
                    "border_right_width": "3px",
                    "border_top_style": "solid",
                    "border_bottom_style": "solid"
                },
                "children": [
                    {
                        "style": {
                            "width": "288px",
                            "display": "block",
                            "height": "110px"
                        },
                        "children": [
                            {
                                "style": {
                                    "width": "300px",
                                    "display": "block",
                                    "height": "150px"
                                },
                                "children": [
                                    {
                                        "style": {
                                            "width": "200px",
                                            "height": "100px"
                                        },
                                        "tag": "svg:rect"
                                    }
                                ],
                                "tag": "svg:svg"
                            }
                        ],
                        "tag": "div"
                    },
                    {
                        "style": {
                            "width": "200px",
                            "display": "block",
                            "height": "96px"
                        },
                        "tag": "div"
                    }
                ],
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "matches": null,
    "assert": "Computed value of 'auto' for 'margin-left' or margin-right' on block replaced elements becomes a used value of '0'. The height and width are adjusted by the constraints for block-level, non-replaced elements."
}