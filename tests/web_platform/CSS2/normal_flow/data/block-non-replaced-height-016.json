{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#normal-block"
    ],
    "test_case": {
        "style": {
            "width": "624px",
            "display": "block",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "96px",
            "margin_right": "8px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "width": "624px",
                    "display": "block",
                    "height": "96px",
                    "position": "relative"
                },
                "children": [
                    {
                        "style": {
                            "width": "96px",
                            "display": "block",
                            "height": "96px"
                        },
                        "children": [
                            {
                                "style": {
                                    "width": "96px",
                                    "height": "96px",
                                    "display": "block",
                                    "top": "96px",
                                    "position": "relative"
                                },
                                "tag": "div"
                            }
                        ],
                        "tag": "div"
                    },
                    {
                        "style": {
                            "width": "96px",
                            "display": "block",
                            "top": "0px",
                            "height": "96px",
                            "left": "96px",
                            "position": "absolute"
                        },
                        "tag": "div"
                    }
                ],
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "matches": null,
    "assert": "A relative positioned child elements' offset is not considered when sizing the 'height' of a block-level non-replaced element is in normal flow."
}