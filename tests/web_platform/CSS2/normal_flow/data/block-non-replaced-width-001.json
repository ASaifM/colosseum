{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#blockwidth"
    ],
    "test_case": {
        "style": {
            "width": "624px",
            "display": "block",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "34px",
            "margin_right": "8px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "display": "inline-block",
                    "border_left_color": "rgb(255, 165, 0)",
                    "border_bottom_style": "solid",
                    "width": "140px",
                    "border_top_width": "5px",
                    "border_bottom_width": "5px",
                    "border_top_color": "rgb(255, 165, 0)",
                    "border_top_style": "solid",
                    "border_right_style": "solid",
                    "border_left_width": "5px",
                    "border_left_style": "solid",
                    "border_bottom_color": "rgb(255, 165, 0)",
                    "height": "20px",
                    "border_right_width": "5px",
                    "border_right_color": "rgb(255, 165, 0)"
                },
                "children": [
                    {
                        "style": {
                            "border_right_style": "solid",
                            "border_left_color": "rgb(255, 165, 0)",
                            "width": "80px",
                            "margin_right": "10px",
                            "display": "block",
                            "padding_left": "10px",
                            "border_left_width": "10px",
                            "padding_right": "10px",
                            "border_left_style": "solid",
                            "height": "20px",
                            "border_right_width": "10px",
                            "border_right_color": "rgb(255, 165, 0)",
                            "margin_left": "10px"
                        },
                        "tag": "div"
                    }
                ],
                "tag": "div"
            },
            {
                "style": {
                    "width": "150px",
                    "height": "30px",
                    "display": "block",
                    "top": "82px",
                    "position": "absolute"
                },
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "matches": "block-non-replaced-width-001-ref.xht",
    "assert": "The equation; 'margin-left' + 'border-left-width' + 'padding-left' + 'width' + 'padding-right' + 'border-right-width' + 'margin-right' is equal the width of containing block."
}