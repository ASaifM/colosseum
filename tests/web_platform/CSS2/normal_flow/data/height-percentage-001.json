{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#propdef-height",
        "http://www.w3.org/TR/CSS21/visudet.html#the-height-property"
    ],
    "test_case": {
        "children": [
            {
                "children": [
                    {
                        "tag": "div",
                        "style": {
                            "display": "block",
                            "height": "96px",
                            "width": "96px"
                        }
                    },
                    {
                        "tag": "div",
                        "style": {
                            "display": "block",
                            "height": "96px",
                            "position": "relative",
                            "width": "96px",
                            "top": "-96px",
                            "left": "96px"
                        }
                    }
                ],
                "tag": "div",
                "style": {
                    "display": "block",
                    "height": "192px",
                    "width": "624px"
                }
            }
        ],
        "tag": "body",
        "style": {
            "margin_top": "8px",
            "display": "block",
            "width": "624px",
            "margin_right": "8px",
            "margin_bottom": "8px",
            "height": "192px",
            "margin_left": "8px"
        }
    },
    "assert": "A percentage height is calculated using the height of the containing block.",
    "matches": "height-percentage-001-ref.xht"
}