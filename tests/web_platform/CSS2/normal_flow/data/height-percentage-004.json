{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#the-height-property"
    ],
    "test_case": {
        "style": {
            "width": "624px",
            "display": "block",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "192px",
            "margin_right": "8px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "width": "192px",
                    "display": "block",
                    "height": "192px",
                    "position": "relative"
                },
                "children": [
                    {
                        "style": {
                            "width": "96px",
                            "display": "block",
                            "height": "96px",
                            "position": "absolute"
                        },
                        "tag": "div"
                    },
                    {
                        "style": {
                            "width": "192px",
                            "display": "block",
                            "height": "192px"
                        },
                        "tag": "div"
                    },
                    {
                        "style": {
                            "width": "96px",
                            "display": "block",
                            "top": "0px",
                            "height": "96px",
                            "left": "96px",
                            "position": "absolute"
                        },
                        "tag": "div"
                    }
                ],
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "matches": "height-percentage-001-ref.xht",
    "assert": "The 'height' of the containing block of an absolutely positioned element is independent of the size of the element itself."
}