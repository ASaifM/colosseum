{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#inline-replaced-height"
    ],
    "test_case": {
        "style": {
            "width": "624px",
            "display": "block",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "15px",
            "margin_right": "8px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "width": "624px",
                    "display": "block",
                    "height": "15px",
                    "position": "relative"
                },
                "children": [
                    {
                        "style": {
                            "width": "15px",
                            "display": "block",
                            "height": "15px"
                        },
                        "tag": "img"
                    },
                    {
                        "style": {
                            "width": "15px",
                            "display": "block",
                            "top": "0px",
                            "height": "15px",
                            "left": "15px",
                            "position": "absolute"
                        },
                        "tag": "div"
                    }
                ],
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "matches": "block-replaced-height-002-ref.xht",
    "assert": "The 'height' is the intrinsic height when an block replaced element with and intrinsic height has a 'height' and 'width' computed as 'auto'."
}