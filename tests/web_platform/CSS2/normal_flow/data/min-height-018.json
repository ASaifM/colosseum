{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#propdef-min-height",
        "http://www.w3.org/TR/CSS21/visudet.html#min-max-heights"
    ],
    "test_case": {
        "style": {
            "width": "624px",
            "display": "block",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "96px",
            "margin_right": "8px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "width": "624px",
                    "display": "block",
                    "height": "96px",
                    "position": "relative"
                },
                "children": [
                    {
                        "style": {
                            "width": "96px",
                            "display": "block",
                            "min_height": "96px",
                            "height": "96px",
                            "position": "relative"
                        },
                        "tag": "div"
                    },
                    {
                        "style": {
                            "width": "96px",
                            "display": "block",
                            "top": "0px",
                            "border_top_width": "96px",
                            "height": "0px",
                            "left": "100px",
                            "border_top_style": "solid",
                            "position": "absolute"
                        },
                        "tag": "div"
                    }
                ],
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "matches": "height-006-ref.xht",
    "assert": "The 'min-height' property sets a positive nominal length value in points."
}