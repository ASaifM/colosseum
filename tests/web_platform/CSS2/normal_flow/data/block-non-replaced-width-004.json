{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#blockwidth"
    ],
    "test_case": {
        "style": {
            "width": "624px",
            "display": "block",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "30px",
            "margin_right": "8px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "width": "624px",
                    "direction": "rtl",
                    "display": "block",
                    "height": "30px",
                    "position": "relative"
                },
                "children": [
                    {
                        "style": {
                            "width": "100px",
                            "direction": "rtl",
                            "display": "block",
                            "height": "30px",
                            "position": "relative"
                        },
                        "children": [
                            {
                                "style": {
                                    "border_right_style": "solid",
                                    "border_left_color": "rgb(255, 165, 0)",
                                    "direction": "rtl",
                                    "height": "30px",
                                    "width": "100px",
                                    "margin_right": "10px",
                                    "display": "block",
                                    "padding_left": "10px",
                                    "border_left_width": "10px",
                                    "padding_right": "10px",
                                    "border_left_style": "solid",
                                    "position": "relative",
                                    "border_right_width": "10px",
                                    "border_right_color": "rgb(255, 165, 0)",
                                    "margin_left": "10px"
                                },
                                "tag": "div"
                            }
                        ],
                        "tag": "div"
                    },
                    {
                        "style": {
                            "width": "150px",
                            "direction": "rtl",
                            "display": "block",
                            "height": "30px",
                            "position": "absolute"
                        },
                        "tag": "div"
                    }
                ],
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "matches": "block-non-replaced-width-004-ref.xht",
    "assert": "When direction is right-to-left, the specified value of 'margin-left' is ignored in an over-constrained situation and the 'margin-left' value is calculated so as to make the equality true."
}