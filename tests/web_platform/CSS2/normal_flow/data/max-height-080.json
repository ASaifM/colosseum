{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#propdef-max-height",
        "http://www.w3.org/TR/CSS21/visudet.html#min-max-heights"
    ],
    "test_case": {
        "style": {
            "width": "624px",
            "display": "block",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "32px",
            "margin_right": "8px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "width": "624px",
                    "display": "block",
                    "max_height": "16px",
                    "height": "16px"
                },
                "tag": "div"
            },
            {
                "style": {
                    "width": "624px",
                    "display": "block",
                    "bottom": "16px",
                    "height": "16px",
                    "position": "relative"
                },
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "matches": "max-height-025-ref.xht",
    "assert": "The 'max-height' property sets a minimum plus one length value in 'ex' units."
}