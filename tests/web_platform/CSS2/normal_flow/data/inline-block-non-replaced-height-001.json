{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#block-root-margin"
    ],
    "test_case": {
        "children": [
            {
                "children": [
                    {
                        "children": [
                            {
                                "tag": "div",
                                "style": {
                                    "display": "block",
                                    "height": "96px",
                                    "width": "96px"
                                }
                            }
                        ],
                        "tag": "div",
                        "style": {
                            "display": "inline-block",
                            "height": "96px",
                            "width": "96px"
                        }
                    },
                    {
                        "tag": "div",
                        "style": {
                            "display": "block",
                            "height": "96px",
                            "position": "absolute",
                            "width": "96px",
                            "top": "0px",
                            "left": "96px"
                        }
                    }
                ],
                "tag": "div",
                "style": {
                    "display": "block",
                    "height": "100px",
                    "position": "relative",
                    "width": "624px"
                }
            }
        ],
        "tag": "body",
        "style": {
            "margin_top": "8px",
            "display": "block",
            "width": "624px",
            "margin_right": "8px",
            "margin_bottom": "8px",
            "height": "100px",
            "margin_left": "8px"
        }
    },
    "assert": "If 'height' is 'auto' on an inline-block non-replaced element then the 'height' depends on the descendants.",
    "matches": "height-percentage-001-ref.xht"
}