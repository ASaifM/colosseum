{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#propdef-height",
        "http://www.w3.org/TR/CSS21/visudet.html#the-height-property"
    ],
    "test_case": {
        "children": [
            {
                "children": [
                    {
                        "tag": "div",
                        "style": {
                            "display": "block",
                            "height": "100px",
                            "position": "relative",
                            "width": "100px"
                        }
                    },
                    {
                        "tag": "div",
                        "style": {
                            "display": "block",
                            "top": "0px",
                            "position": "absolute",
                            "width": "100px",
                            "border_top_width": "100px",
                            "left": "104px",
                            "height": "0px",
                            "border_top_style": "solid"
                        }
                    }
                ],
                "tag": "div",
                "style": {
                    "display": "block",
                    "height": "100px",
                    "position": "relative",
                    "width": "624px"
                }
            }
        ],
        "tag": "body",
        "style": {
            "margin_top": "8px",
            "display": "block",
            "width": "624px",
            "margin_right": "8px",
            "margin_bottom": "8px",
            "height": "100px",
            "margin_left": "8px"
        }
    },
    "assert": "The 'height' property sets a nominal length value in 'em' units.",
    "matches": "height-072-ref.xht"
}