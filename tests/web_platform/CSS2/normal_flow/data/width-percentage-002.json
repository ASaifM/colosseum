{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#the-width-property"
    ],
    "test_case": {
        "children": [
            {
                "tag": "div",
                "style": {
                    "display": "block",
                    "height": "96px",
                    "width": "96px"
                }
            },
            {
                "children": [
                    {
                        "children": [
                            {
                                "tag": "div",
                                "style": {
                                    "display": "block",
                                    "height": "96px",
                                    "position": "absolute",
                                    "width": "96px",
                                    "top": "0px",
                                    "left": "0px"
                                }
                            }
                        ],
                        "tag": "div",
                        "style": {
                            "padding_right": "48px",
                            "display": "block",
                            "position": "absolute",
                            "width": "96px",
                            "padding_left": "48px",
                            "top": "0px",
                            "left": "0px",
                            "height": "96px"
                        }
                    }
                ],
                "tag": "div",
                "style": {
                    "display": "block",
                    "height": "0px",
                    "position": "relative",
                    "width": "624px"
                }
            }
        ],
        "tag": "body",
        "style": {
            "margin_top": "8px",
            "display": "block",
            "width": "624px",
            "margin_right": "8px",
            "margin_bottom": "8px",
            "height": "96px",
            "margin_left": "8px"
        }
    },
    "assert": "Absolutely positioned elements whose containing block is based on a block-level element, the percentage is calculated with respect to the width of the padding box of that element.",
    "matches": "max-width-percentage-001-ref.xht"
}