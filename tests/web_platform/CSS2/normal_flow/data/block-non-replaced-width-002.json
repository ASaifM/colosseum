{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#blockwidth"
    ],
    "test_case": {
        "style": {
            "width": "624px",
            "display": "block",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "30px",
            "margin_right": "8px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "width": "100px",
                    "display": "block",
                    "margin_top": "15px",
                    "height": "30px"
                },
                "children": [
                    {
                        "style": {
                            "display": "block",
                            "border_left_color": "rgb(255, 165, 0)",
                            "border_right_style": "solid",
                            "padding_left": "10px",
                            "border_left_width": "10px",
                            "padding_right": "10px",
                            "width": "110px",
                            "border_left_style": "solid",
                            "height": "30px",
                            "border_right_width": "10px",
                            "border_right_color": "rgb(255, 165, 0)"
                        },
                        "tag": "div"
                    }
                ],
                "tag": "div"
            },
            {
                "style": {
                    "width": "150px",
                    "height": "30px",
                    "display": "block",
                    "top": "82px",
                    "position": "absolute"
                },
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "matches": null,
    "assert": "When width is auto and the combination of border widths, padding widths, scrollbar widths, width, and non 'auto' margin widths are larger than the width of the containing block, the margin widths are treated as 0."
}