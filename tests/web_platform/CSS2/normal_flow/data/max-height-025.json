{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#propdef-max-height",
        "http://www.w3.org/TR/CSS21/visudet.html#min-max-heights"
    ],
    "test_case": {
        "children": [
            {
                "tag": "div",
                "style": {
                    "display": "block",
                    "height": "16px",
                    "width": "624px",
                    "max_height": "16px"
                }
            },
            {
                "tag": "div",
                "style": {
                    "display": "block",
                    "width": "624px",
                    "position": "relative",
                    "bottom": "16px",
                    "height": "16px"
                }
            }
        ],
        "tag": "body",
        "style": {
            "margin_top": "8px",
            "display": "block",
            "width": "624px",
            "margin_right": "8px",
            "margin_bottom": "8px",
            "height": "32px",
            "margin_left": "8px"
        }
    },
    "assert": "The 'max-height' property sets a minimum plus one length value in picas.",
    "matches": "max-height-025-ref.xht"
}