{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#inlineblock-replaced-width"
    ],
    "test_case": {
        "style": {
            "width": "624px",
            "display": "block",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "198px",
            "margin_right": "8px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "border_right_style": "solid",
                    "width": "192px",
                    "border_top_width": "3px",
                    "border_left_width": "3px",
                    "height": "192px",
                    "border_left_style": "solid",
                    "display": "block",
                    "border_bottom_width": "3px",
                    "border_right_width": "3px",
                    "border_top_style": "solid",
                    "border_bottom_style": "solid"
                },
                "children": [
                    {
                        "style": {
                            "width": "15px",
                            "display": "inline-block",
                            "height": "15px"
                        },
                        "tag": "img"
                    },
                    {
                        "style": {
                            "width": "15px",
                            "display": "block",
                            "height": "15px"
                        },
                        "tag": "div"
                    }
                ],
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "matches": "inline-block-replaced-width-001-ref.xht",
    "assert": "Computed value of 'auto' for 'margin-left' or margin-right' on inline-block replaced elements becomes a used value of '0'. The intrinsic width is also used if 'height' and 'width' are 'auto'."
}