{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#propdef-max-width",
        "http://www.w3.org/TR/CSS21/visudet.html#min-max-widths"
    ],
    "test_case": {
        "style": {
            "width": "624px",
            "display": "block",
            "margin_top": "8px",
            "margin_bottom": "8px",
            "height": "96px",
            "margin_right": "8px",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "width": "288px",
                    "display": "table",
                    "height": "96px"
                },
                "children": [
                    {
                        "style": {
                            "width": "0px",
                            "display": "table-column",
                            "max_width": "96px",
                            "height": "0px"
                        },
                        "tag": "div"
                    },
                    {
                        "style": {
                            "width": "288px",
                            "display": "table-row",
                            "height": "96px"
                        },
                        "children": [
                            {
                                "style": {
                                    "width": "288px",
                                    "display": "table-cell",
                                    "height": "96px"
                                },
                                "tag": "div"
                            }
                        ],
                        "tag": "div"
                    }
                ],
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "matches": "../../reference/pass_if_square_96px_black.html",
    "assert": "The 'max-width' property applies to elements with 'display' set to 'table-column'."
}