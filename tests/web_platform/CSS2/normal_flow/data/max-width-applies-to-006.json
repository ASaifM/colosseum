{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#propdef-max-width",
        "http://www.w3.org/TR/CSS21/visudet.html#min-max-widths"
    ],
    "test_case": {
        "children": [
            {
                "children": [
                    {
                        "tag": "div",
                        "style": {
                            "display": "table-column",
                            "max_width": "96px",
                            "width": "0px",
                            "height": "0px"
                        }
                    },
                    {
                        "children": [
                            {
                                "tag": "div",
                                "style": {
                                    "display": "table-cell",
                                    "height": "96px",
                                    "width": "288px"
                                }
                            }
                        ],
                        "tag": "div",
                        "style": {
                            "display": "table-row",
                            "height": "96px",
                            "width": "288px"
                        }
                    }
                ],
                "tag": "div",
                "style": {
                    "display": "table",
                    "height": "96px",
                    "width": "288px"
                }
            }
        ],
        "tag": "body",
        "style": {
            "margin_top": "8px",
            "display": "block",
            "width": "624px",
            "margin_right": "8px",
            "margin_bottom": "8px",
            "height": "96px",
            "margin_left": "8px"
        }
    },
    "assert": "The 'max-width' property applies to elements with 'display' set to 'table-column'.",
    "matches": "../../reference/pass_if_square_96px_black.html"
}