{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#propdef-min-height",
        "http://www.w3.org/TR/CSS21/visudet.html#min-max-heights"
    ],
    "test_case": {
        "children": [
            {
                "children": [
                    {
                        "tag": "div",
                        "style": {
                            "display": "block",
                            "height": "1px",
                            "width": "624px",
                            "min_height": "1px"
                        }
                    }
                ],
                "tag": "div",
                "style": {
                    "display": "block",
                    "height": "1px",
                    "width": "624px",
                    "min_height": "1px"
                }
            }
        ],
        "tag": "body",
        "style": {
            "margin_top": "8px",
            "display": "block",
            "width": "624px",
            "margin_right": "8px",
            "margin_bottom": "8px",
            "height": "1px",
            "margin_left": "8px"
        }
    },
    "assert": "The 'min-height' property properly inherits the min-height value of the parent.",
    "matches": "height-003-ref.xht"
}