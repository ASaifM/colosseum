{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#propdef-max-width",
        "http://www.w3.org/TR/CSS21/visudet.html#min-max-widths"
    ],
    "test_case": {
        "children": [
            {
                "children": [
                    {
                        "tag": "span",
                        "style": {
                            "display": "block",
                            "max_width": "96px",
                            "width": "96px",
                            "height": "96px"
                        }
                    }
                ],
                "tag": "div",
                "style": {
                    "display": "block",
                    "height": "96px",
                    "width": "624px"
                }
            }
        ],
        "tag": "body",
        "style": {
            "margin_top": "8px",
            "display": "block",
            "width": "624px",
            "margin_right": "8px",
            "margin_bottom": "8px",
            "height": "96px",
            "margin_left": "8px"
        }
    },
    "assert": "The 'max-width' property applies to elements with 'display' set to 'block'.",
    "matches": "../reference/ref-filled-black-96px-square.xht"
}