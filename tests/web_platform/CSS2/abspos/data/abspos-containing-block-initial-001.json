{
    "help": [
        "http://www.w3.org/TR/CSS21/visudet.html#containing-block-details"
    ],
    "assert": "If there is no such ancestor, the containing block is the initial containing block.",
    "test_case": {
        "tag": "body",
        "children": [
            {
                "tag": "script",
                "children": [
                    {
                        "text": "window.scrollTo(0,50);",
                        "style": {}
                    }
                ],
                "style": {
                    "display": "none"
                }
            },
            {
                "tag": "div",
                "style": {
                    "top": "0px",
                    "display": "block",
                    "width": "100px",
                    "height": "100px",
                    "position": "absolute"
                }
            },
            {
                "tag": "div",
                "style": {
                    "display": "block",
                    "width": "100px",
                    "right": "0px",
                    "height": "100px",
                    "position": "absolute"
                }
            },
            {
                "tag": "div",
                "style": {
                    "display": "block",
                    "width": "100px",
                    "bottom": "0px",
                    "height": "100px",
                    "position": "absolute"
                }
            },
            {
                "tag": "div",
                "style": {
                    "display": "block",
                    "left": "0px",
                    "height": "100px",
                    "width": "100px",
                    "position": "absolute"
                }
            }
        ],
        "style": {
            "display": "block",
            "width": "520px",
            "height": "10000px"
        }
    },
    "matches": "abspos-containing-block-initial-001-ref.xht"
}