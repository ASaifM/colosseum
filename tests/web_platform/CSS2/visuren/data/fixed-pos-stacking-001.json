                "children": [
                    {
                        "tag": "div",
                        "style": {
                            "display": "block",
                            "position": "absolute",
                            "width": "80px",
                            "height": "32px"
                        },
                        "children": [
                            {
                                "tag": "div",
                                "style": {
                                    "display": "block",
                                    "position": "absolute",
                                    "width": "80px",
                                    "z_index": "-1",
                                    "height": "32px"
                                }
                            }
                        ]
                    }
                ]
