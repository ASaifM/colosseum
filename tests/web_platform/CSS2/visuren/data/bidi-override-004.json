{
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#propdef-unicode-bidi",
        "http://www.w3.org/TR/CSS21/visuren.html#direction"
    ],
    "assert": "bidi-override should be applied when specified on list item",
    "test_case": {
        "tag": "body",
        "style": {
            "margin_top": "8px",
            "margin_right": "8px",
            "height": "18px",
            "margin_bottom": "8px",
            "width": "624px",
            "margin_left": "8px",
            "display": "block"
        },
        "children": [
            {
                "tag": "ul",
                "style": {
                    "margin_top": "16px",
                    "margin_right": "32px",
                    "height": "18px",
                    "margin_bottom": "16px",
                    "width": "560px",
                    "margin_left": "32px",
                    "display": "block"
                },
                "children": [
                    {
                        "tag": "li",
                        "style": {
                            "margin_top": "16px",
                            "margin_right": "32px",
                            "direction": "rtl",
                            "height": "18px",
                            "margin_bottom": "16px",
                            "width": "496px",
                            "margin_left": "32px",
                            "display": "list-item",
                            "unicode_bidi": "bidi-override"
                        },
                        "children": [
                            {
                                "text": "SSAP",
                                "style": {}
                            }
                        ]
                    }
                ]
            }
        ]
    },
    "matches": null
}