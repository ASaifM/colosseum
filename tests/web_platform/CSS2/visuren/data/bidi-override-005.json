{
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#propdef-unicode-bidi",
        "http://www.w3.org/TR/CSS21/visuren.html#direction"
    ],
    "assert": "bidi-override should be applied to inline-level descendants but not block-level descendants",
    "test_case": {
        "tag": "body",
        "style": {
            "margin_top": "8px",
            "margin_right": "8px",
            "height": "36px",
            "margin_bottom": "8px",
            "width": "624px",
            "margin_left": "8px",
            "display": "block"
        },
        "children": [
            {
                "tag": "div",
                "style": {
                    "height": "36px",
                    "direction": "rtl",
                    "width": "624px",
                    "unicode_bidi": "bidi-override",
                    "display": "block"
                },
                "children": [
                    {
                        "text": "cba",
                        "style": {}
                    },
                    {
                        "tag": "div",
                        "style": {
                            "height": "18px",
                            "direction": "rtl",
                            "width": "624px",
                            "display": "block"
                        },
                        "children": [
                            {
                                "text": "abc",
                                "style": {}
                            }
                        ]
                    }
                ]
            }
        ]
    },
    "matches": null
}