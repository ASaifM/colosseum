{
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#dis-pos-flo"
    ],
    "assert": "Float is computed to 'none' when 'position: absolute' is specified.",
    "test_case": {
        "tag": "body",
        "style": {
            "margin_top": "8px",
            "margin_right": "8px",
            "height": "100px",
            "margin_bottom": "8px",
            "width": "624px",
            "margin_left": "8px",
            "display": "block"
        },
        "children": [
            {
                "tag": "div",
                "style": {
                    "height": "100px",
                    "position": "relative",
                    "width": "624px",
                    "display": "block"
                },
                "children": [
                    {
                        "tag": "div",
                        "style": {
                            "height": "100px",
                            "position": "absolute",
                            "width": "100px",
                            "display": "block"
                        }
                    },
                    {
                        "tag": "div",
                        "style": {
                            "height": "100px",
                            "width": "100px",
                            "display": "block"
                        }
                    }
                ]
            }
        ]
    },
    "matches": "../reference/ref-filled-green-100px-square.xht"
