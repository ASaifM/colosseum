{
    "help": [
        "http://www.w3.org/TR/CSS21/visuren.html#position-props",
        "http://www.w3.org/TR/CSS21/cascade.html#value-def-inherit",
        "http://www.w3.org/TR/css-cascade-3/#inherit",
        "http://www.w3.org/TR/css-cascade-4/#inherit"
    ],
    "assert": "'top: inherit' makes the top property take the same computed value as the top property for the element's parent; in the case of a percentage value, the computed value is the specified percentage value. 'top: [percentage]' refers to height of containing block.",
    "test_case": {
        "tag": "body",
        "style": {
            "margin_top": "8px",
            "margin_right": "8px",
            "height": "400px",
            "margin_bottom": "8px",
            "width": "624px",
            "margin_left": "8px",
            "display": "block"
        },
        "children": [
            {
                "tag": "div",
                "style": {
                    "height": "400px",
                    "position": "relative",
                    "width": "600px",
                    "display": "block"
                },
                "children": [
                    {
                        "tag": "div",
                        "style": {
                            "top": "50px",
                            "left": "300px",
                            "position": "relative",
                            "height": "100px",
                            "border_bottom_color": "rgb(255, 255, 255)",
                            "width": "100px",
                            "border_left_color": "rgb(255, 255, 255)",
                            "border_top_color": "rgb(255, 255, 255)",
                            "border_right_color": "rgb(255, 255, 255)",
                            "display": "block"
                        },
                        "children": [
                            {
                                "text": "test FAILED",
                                "style": {}
                            }
                        ]
                    },
                    {
                        "tag": "div",
                        "style": {
                            "top": "-12.5%",
                            "left": "50%",
                            "position": "relative",
                            "height": "0px",
                            "width": "100px",
                            "display": "block"
                        },
                        "children": [
                            {
                                "tag": "div",
                                "style": {
                                    "top": "-12.5%",
                                    "left": "0px",
                                    "position": "relative",
                                    "height": "100px",
                                    "width": "100px",
                                    "display": "block"
                                }
                            }
                        ]
                    }
                ]
            }
        ]
    },
    "matches": "top-114-ref.xht"
}