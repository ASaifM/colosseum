{
    "help": [
        "http://www.w3.org/TR/css-flexbox-1/#flex-property"
    ],
    "matches": "reference/flex-vertical-align-effect.html",
    "test_case": {
        "children": [
            {
                "children": [
                    {
                        "style": {
                            "border_right_width": "2px",
                            "min_height": "auto",
                            "height": "13px",
                            "border_right_style": "inset",
                            "border_bottom_style": "inset",
                            "display": "block",
                            "border_left_style": "inset",
                            "border_top_width": "2px",
                            "border_top_style": "inset",
                            "width": "139.875px",
                            "border_bottom_width": "2px",
                            "border_left_width": "2px",
                            "min_width": "auto"
                        },
                        "tag": "INPUT"
                    },
                    {
                        "style": {
                            "width": "12px",
                            "height": "12px",
                            "display": "block"
                        },
                        "tag": "INPUT"
                    }
                ],
                "style": {
                    "width": "624px",
                    "height": "17px",
                    "display": "flex"
                },
                "tag": "DIV"
            }
        ],
        "style": {
            "margin_left": "8px",
            "height": "17px",
            "margin_right": "8px",
            "display": "block",
            "width": "624px",
            "margin_top": "8px",
            "margin_bottom": "8px"
        },
        "tag": "BODY"
    },
    "assert": "vertical-align property has no effect"
}