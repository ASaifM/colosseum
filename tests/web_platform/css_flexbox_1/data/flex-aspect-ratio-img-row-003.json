{
    "help": [
        "https://drafts.csswg.org/css-flexbox-1/#algo-cross-item"
    ],
    "matches": "../reference/ref-filled-green-100px-square.xht",
    "test_case": {
        "children": [
            {
                "style": {
                    "width": "100px",
                    "position": "absolute",
                    "z_index": "-1",
                    "height": "100px",
                    "display": "block"
                },
                "tag": "DIV"
            },
            {
                "children": [
                    {
                        "style": {
                            "width": "100px",
                            "height": "200px",
                            "display": "block"
                        },
                        "tag": "IMG"
                    }
                ],
                "style": {
                    "width": "10px",
                    "height": "200px",
                    "display": "flex"
                },
                "tag": "DIV"
            }
        ],
        "style": {
            "margin_left": "8px",
            "height": "200px",
            "margin_right": "8px",
            "display": "block",
            "width": "624px",
            "margin_top": "8px",
            "margin_bottom": "8px"
        },
        "tag": "BODY"
    },
    "assert": "Test that we compute the correct aspect-ratio based cross size when a flex-basis is specified"
}