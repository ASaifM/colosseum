{
    "assert": "Whitespace-only nodes in a flexbox should not\nbe rendered and not influence justify-content, even in the presence of\nwhite-space: pre",
    "test_case": {
        "style": {
            "margin_right": "8px",
            "margin_top": "8px",
            "width": "624px",
            "height": "72px",
            "margin_bottom": "8px",
            "display": "block",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "height": "36px",
                    "width": "624px",
                    "display": "flex"
                },
                "children": [
                    {
                        "style": {
                            "height": "36px",
                            "min_width": "auto",
                            "width": "30px",
                            "display": "block",
                            "min_height": "auto"
                        },
                        "tag": "DIV"
                    }
                ],
                "tag": "DIV"
            },
            {
                "style": {
                    "height": "36px",
                    "width": "624px",
                    "display": "flex"
                },
                "children": [
                    {
                        "style": {
                            "height": "36px",
                            "min_width": "auto",
                            "width": "21.328125px",
                            "display": "block",
                            "min_height": "auto"
                        },
                        "children": [
                            {
                                "style": {},
                                "text": "foo"
                            }
                        ],
                        "tag": "B"
                    },
                    {
                        "style": {
                            "height": "36px",
                            "min_width": "auto",
                            "width": "24px",
                            "display": "block",
                            "min_height": "auto"
                        },
                        "children": [
                            {
                                "style": {},
                                "text": "bar"
                            }
                        ],
                        "tag": "B"
                    }
                ],
                "tag": "DIV"
            }
        ],
        "tag": "BODY"
    },
    "help": [
        "https://drafts.csswg.org/css-flexbox/#flex-items"
    ],
    "matches": "whitespace-in-flexitem-001-ref.html"
}