{
    "help": [
        "https://drafts.csswg.org/css-flexbox/#flex-items"
    ],
    "matches": "whitespace-in-flexitem-001-ref.html",
    "test_case": {
        "children": [
            {
                "children": [
                    {
                        "style": {
                            "width": "30px",
                            "min_width": "auto",
                            "min_height": "auto",
                            "height": "36px",
                            "display": "block"
                        },
                        "tag": "DIV"
                    }
                ],
                "style": {
                    "width": "624px",
                    "height": "36px",
                    "display": "flex"
                },
                "tag": "DIV"
            },
            {
                "children": [
                    {
                        "children": [
                            {
                                "text": "foo",
                                "style": {}
                            }
                        ],
                        "style": {
                            "width": "21.328125px",
                            "min_width": "auto",
                            "min_height": "auto",
                            "height": "36px",
                            "display": "block"
                        },
                        "tag": "B"
                    },
                    {
                        "children": [
                            {
                                "text": "bar",
                                "style": {}
                            }
                        ],
                        "style": {
                            "width": "24px",
                            "min_width": "auto",
                            "min_height": "auto",
                            "height": "36px",
                            "display": "block"
                        },
                        "tag": "B"
                    }
                ],
                "style": {
                    "width": "624px",
                    "height": "36px",
                    "display": "flex"
                },
                "tag": "DIV"
            }
        ],
        "style": {
            "margin_left": "8px",
            "height": "72px",
            "margin_right": "8px",
            "display": "block",
            "width": "624px",
            "margin_top": "8px",
            "margin_bottom": "8px"
        },
        "tag": "BODY"
    },
    "assert": "Whitespace-only nodes in a flexbox should not\nbe rendered and not influence justify-content, even in the presence of\nwhite-space: pre"
}