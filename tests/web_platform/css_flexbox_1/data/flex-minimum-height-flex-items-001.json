{
    "help": [
        "http://www.w3.org/TR/css-flexbox-1/#min-size-auto"
    ],
    "matches": "../reference/ref-filled-green-100px-square.xht",
    "test_case": {
        "children": [
            {
                "style": {
                    "width": "100px",
                    "position": "absolute",
                    "z_index": "-1",
                    "height": "100px",
                    "display": "block"
                },
                "tag": "div"
            },
            {
                "children": [
                    {
                        "children": [
                            {
                                "style": {
                                    "border_right_color": "rgb(0, 128, 0)",
                                    "border_left_color": "rgb(0, 128, 0)",
                                    "height": "100px",
                                    "display": "block",
                                    "width": "100px",
                                    "border_top_color": "rgb(0, 128, 0)",
                                    "border_bottom_color": "rgb(0, 128, 0)"
                                },
                                "tag": "div"
                            }
                        ],
                        "style": {
                            "min_height": "auto",
                            "border_right_color": "rgb(0, 128, 0)",
                            "border_left_color": "rgb(0, 128, 0)",
                            "height": "100px",
                            "display": "block",
                            "width": "100px",
                            "border_top_color": "rgb(0, 128, 0)",
                            "border_bottom_color": "rgb(0, 128, 0)",
                            "min_width": "auto"
                        },
                        "tag": "div"
                    }
                ],
                "style": {
                    "width": "100px",
                    "height": "10px",
                    "display": "flex"
                },
                "tag": "div"
            }
        ],
        "style": {
            "margin_left": "8px",
            "height": "10px",
            "margin_right": "8px",
            "display": "block",
            "width": "624px",
            "margin_top": "8px",
            "margin_bottom": "8px"
        },
        "tag": "body"
    },
    "assert": "Checks that minimum height for flex items is the min-content size."
}