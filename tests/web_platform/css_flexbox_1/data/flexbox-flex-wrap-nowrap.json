{
    "help": [
        "http://www.w3.org/TR/css-flexbox-1/#flex-wrap-property"
    ],
    "matches": "reference/flexbox-flex-wrap-nowrap-ref.htm",
    "test_case": {
        "children": [
            {
                "children": [
                    {
                        "style": {
                            "width": "60px",
                            "min_width": "auto",
                            "min_height": "auto",
                            "height": "120px",
                            "display": "block"
                        },
                        "tag": "DIV"
                    },
                    {
                        "style": {
                            "width": "60px",
                            "min_width": "auto",
                            "min_height": "auto",
                            "height": "120px",
                            "display": "block"
                        },
                        "tag": "DIV"
                    }
                ],
                "style": {
                    "border_right_color": "rgb(255, 0, 0)",
                    "height": "120px",
                    "border_right_style": "solid",
                    "display": "flex",
                    "width": "60px",
                    "border_right_width": "60px"
                },
                "tag": "DIV"
            }
        ],
        "style": {
            "margin_left": "8px",
            "height": "120px",
            "margin_right": "8px",
            "display": "block",
            "width": "624px",
            "margin_top": "8px",
            "margin_bottom": "8px"
        },
        "tag": "BODY"
    },
    "assert": "Test checks that flex elements set to flex-wrap: nowrap will not wrap their flex items. With wrapping disabled, the .green flex item should extend outside the bounds of its container, as it is set to flex:none."
}