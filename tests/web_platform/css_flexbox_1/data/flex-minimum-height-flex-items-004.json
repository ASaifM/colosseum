{
    "assert": "Checks that minimum height for flex items is the min-content size (which corresponds to the image size).",
    "test_case": {
        "style": {
            "margin_right": "8px",
            "margin_top": "8px",
            "width": "624px",
            "height": "10px",
            "margin_bottom": "8px",
            "display": "block",
            "margin_left": "8px"
        },
        "children": [
            {
                "style": {
                    "height": "100px",
                    "z_index": "-1",
                    "width": "100px",
                    "display": "block",
                    "position": "absolute"
                },
                "tag": "div"
            },
            {
                "style": {
                    "height": "10px",
                    "width": "100px",
                    "display": "flex"
                },
                "children": [
                    {
                        "style": {
                            "height": "100px",
                            "min_width": "auto",
                            "width": "100px",
                            "display": "block",
                            "min_height": "auto"
                        },
                        "tag": "img"
                    }
                ],
                "tag": "div"
            }
        ],
        "tag": "body"
    },
    "help": [
        "http://www.w3.org/TR/css-flexbox-1/#min-size-auto"
    ],
    "matches": "../reference/ref-filled-green-100px-square.xht"
