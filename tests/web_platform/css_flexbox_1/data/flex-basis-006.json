{
    "test_case": {
        "tag": "BODY",
        "style": {
            "margin_bottom": "8px",
            "margin_left": "8px",
            "margin_top": "8px",
            "width": "624px",
            "display": "block",
            "margin_right": "8px",
            "height": "100px"
        },
        "children": [
            {
                "tag": "DIV",
                "style": {
                    "width": "100px",
                    "display": "flex",
                    "height": "100px"
                },
                "children": [
                    {
                        "tag": "DIV",
                        "style": {
                            "min_height": "auto",
                            "min_width": "auto",
                            "width": "0px",
                            "display": "block",
                            "height": "100px"
                        }
                    }
                ]
            }
        ]
    },
    "help": [
        "http://www.w3.org/TR/css-flexbox-1/#flex-basis-property"
    ],
    "matches": "../reference/ref-filled-green-100px-square.xht",
    "assert": "The 'flex-basis' property set '0%', the actual width of tested element is same as 0."
}